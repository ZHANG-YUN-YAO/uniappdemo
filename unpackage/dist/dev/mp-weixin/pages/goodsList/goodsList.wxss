.list_box {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-flex-wrap: nowrap;
          flex-wrap: nowrap;
  -webkit-box-pack: start;
  -webkit-justify-content: flex-start;
          justify-content: flex-start;
  -webkit-box-align: start;
  -webkit-align-items: flex-start;
          align-items: flex-start;
  -webkit-align-content: flex-start;
          align-content: flex-start;
  font-size: 28rpx;
}
.list_box .left {
    width: 186rpx;
    background-color: #f6f6f6;
    line-height: 80rpx;
    box-sizing: border-box;
    font-size: 32rpx;
    text-align: center;
}
.list_box .left .item {
      padding-left: 20rpx;
      position: relative;
      width: 144rpx;
      height: 47rpx;
      line-height: 47rpx;
      border-radius: 24rpx;
      margin: 51rpx auto;
      padding-left: 0;
      font-size: 25rpx;
      font-family: Microsoft YaHei;
      font-weight: 400;
}
.list_box .left .item:not(:first-child) {
        margin-top: 1px;
}
.list_box .left .item:not(:first-child)::after {
          content: '';
          display: block;
          height: 0;
          width: 620rpx;
          position: absolute;
          top: -1px;
          right: 0;
          -webkit-transform: scaleY(0.5);
                  transform: scaleY(0.5);
          /* 1px像素 */
}
.list_box .left .item.active {
        color: #FFFFFF;
        background-color: #2852F2;
}
.list_box .left .fill-last {
      height: 0;
      width: 100%;
      background: none;
}
.list_box .main {
    background-color: #fff;
    padding-left: 21rpx;
    padding-right: 28rpx;
    width: 0;
    -webkit-box-flex: 1;
    -webkit-flex-grow: 1;
            flex-grow: 1;
    box-sizing: border-box;
}
.list_box .main .item {
      padding-bottom: 10rpx;
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-pack: justify;
      -webkit-justify-content: space-between;
              justify-content: space-between;
}
.list_box .main .goods {
      display: -webkit-box;
      display: -webkit-flex;
      display: flex;
      -webkit-box-orient: vertical;
      -webkit-box-direction: normal;
      -webkit-flex-direction: column;
              flex-direction: column;
      -webkit-flex-wrap: nowrap;
              flex-wrap: nowrap;
      -webkit-box-pack: start;
      -webkit-justify-content: flex-start;
              justify-content: flex-start;
      -webkit-box-align: center;
      -webkit-align-items: center;
              align-items: center;
      -webkit-align-content: center;
              align-content: center;
      margin-bottom: 10rpx;
      padding: 7rpx;
}
.list_box .main .goods > image {
        width: 250rpx;
        height: 181rpx;
        border-radius: 11rpx;
}
.list_box .main .goods > .goodsnav {
        width: 250rpx;
}
.list_box .main .goods .describe {
        font-size: 22rpx;
        font-family: Microsoft YaHei;
        font-weight: 400;
        color: #161e49;
        line-height: 31rpx;
        padding: 14rpx 4rpx 1rpx 4rpx;
        text-overflow: ellipsis;
        overflow: hidden;
        display: -webkit-box;
        -webkit-line-clamp: 2;
        -webkit-box-orient: vertical;
}
.list_box .main .goods .money {
        font-size: 24rpx;
        color: #FF2829;
        line-height: 37rpx;
}
.list_box .main .goods .sellnum {
        font-size: 19rpx;
        font-family: FZLTZHUNHK;
        font-weight: normal;
        color: #999999;
        line-height: 13rpx;
}

